<h2>Create a project, and slave for the motherland.</h2>
<div class="container-form">
    <form>
        <div class="row">
            <div class="col-25">
                <label for="projectName">Project Name : </label>
            </div>
            <div class="col-75">
                <input type="text" class="projectName" name="project-name" placeholder="Enter New Project Name">
            </div>
        </div>
        <div class="row">
            <div class="col-25">
                <label for="description">Project Description : </label>
            </div>
            <div class="col-75">
                <textarea class="description" name="subject" placeholder="Please Describe"
                    style="height:200px"></textarea>
            </div>
        </div>
        <div class="row">
            <div class="col-25">
                <label for="description">Due Date : </label>
            </div>
            <input type="date" id="due-date" name="due-date" value="">

        </div>
        <div class="row">
            <div class="col-25">
                <label for="teammates">Choose Your Members :</label>
            </div>
            <input type="teammates" id="team" name="teammates" value="">

        </div>
        <div class="row">
            <input type="submit" value="Submit">
        </div>
    </form>
</div>



{{> navbar}}